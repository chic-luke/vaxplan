<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="it.vaxplan.frontend.controller.RegistrationSecondScreenController">
   <children>
      <VBox layoutX="-2.0" prefHeight="400.0" prefWidth="607.0">
         <children>
            <HBox prefHeight="109.0" prefWidth="600.0">
               <children>
                  <Region HBox.hgrow="ALWAYS" />
                  <Label text="Registrazione completata!">
                     <font>
                        <Font size="36.0" />
                     </font>
                     <HBox.margin>
                        <Insets top="30.0" />
                     </HBox.margin>
                  </Label>
                  <Region HBox.hgrow="ALWAYS" />
               </children></HBox>
            <HBox>
               <children>
                  <Label text="Fa parte delle seguenti categorie di pazienti:">
                     <font>
                        <Font size="18.0" />
                     </font>
                     <HBox.margin>
                        <Insets bottom="10.0" left="20.0" />
                     </HBox.margin>
                  </Label>
               </children></HBox>
            <HBox prefHeight="218.0" prefWidth="607.0">
               <children>
                  <ListView fx:id="categoriesList" prefHeight="218.0" prefWidth="609.0">
                     <HBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </HBox.margin>
                  </ListView>
               </children>
            </HBox>
            <ToolBar prefHeight="40.0" prefWidth="582.0">
               <items>
                  <Region prefHeight="29.0" prefWidth="470.0" />
                  <Button fx:id="confirmButton" mnemonicParsing="false" onAction="#confirmButtonAction" prefHeight="24.0" prefWidth="117.0" text="Avanti" />
               </items>
            </ToolBar>
         </children>
      </VBox>
   </children>
</AnchorPane>
